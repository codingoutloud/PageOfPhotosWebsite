@model List<PageOfPhotosWebsite.Models.PageRef>

@{
    ViewData["Title"] = "Current Users";
}

<h2>Anyone can READ pageofphotos.com.</h2>
<h3>To get YOUR OWN PAGE, <a target="_blank" href="https://pageofphotos-registration-twitter.azurewebsites.net/Home/About">click here</a> (NOTE: you will need to authenticate with Twitter)</h3>

<fieldset>
    <legend>Members</legend>
    <ol>
        @foreach (var item in Model)
            {
            <li><img src="@item.UserAvatarUrl" height="25" /><a href="/Home/Page/@item.Slug">@item.Name (@item.Slug)</a></li>
        }
    </ol>
</fieldset>
